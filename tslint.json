{
  "extends": ["tslint:latest", "tslint-react"],
  "rules": {
    "semicolon": [true, "never"],
    "trailing-comma": [true, {"multiline": "never", "singleline": "never"}],
    "object-literal-sort-keys": [false, "never"],
    "no-submodule-imports": false,
    "jsx-no-lambda": false,
    "jsx-no-multiline-js": false
  }
}